{
  "files.exclude": {
    ".env": true,
    "config-overrides.js": true,
    ".gitignore": true,
    "Readme.md": true,
    "tsconfig.json": true,
    ".vscode": true,
    "public": true,
    "node_modules": true,
    "package-lock.json": true,
    "package.json": true,
    "src/react-app-env.d.ts": true,
    "src/index.d.ts": true,
    "src/index.css": true,
    "src/App.css": true,
    "src/controller/exercises/extractor.py": true,
    "src/controller/exercises/converter.py": true,
    "src/controller/exercises/Exercises.txt": true,
    "src/view/components": false,
    "src/view/screens/navigation": true,
    "src/model/muscles": true,
    "src/model/schedule": true,
    "src/_database/schedulesDB/MetaIDs.txt": false,
    "src/_database/schedulesDB/MetaIDExtractor.py": false,
    "src/_application": true,
    "src/model/ExercisesModel.d.ts": true,
    "src/controller/exercises": true,
    "src/index.tsx": true,
    "src/model": false,
    "src/view/containers": true,
    "src/_database": true,
    "src/view/styles": true,
    "src/Helpers.tsx": true,
    "src/Constants.tsx": true,
    "src/view/ScheduleBoardScreen.tsx": true,
    "src/view/components/TypographyElements.tsx": true
  },
  "editor.defaultFormatter": "rvest.vs-code-prettier-eslint",
  "editor.formatOnPaste": false, // required
  "editor.formatOnType": false, // required
  "editor.formatOnSave": true, // optional
  "editor.formatOnSaveMode": "file", // required to format on save
  "files.autoSave": "onFocusChange", // optional but recommended
  "vs-code-prettier-eslint.prettierLast": false // set as "true" to run 'prettier' last not first
}
